create table sll(
eid number(5),
ename varchar(20),
age number(5),
salary number(10)
);
desc sll;

insert into sll values(101,'ram',23,8483);
insert into sll values(102,'shyam',78,3211);
insert into sll values(103,'kate',71,8992);
insert into sll values(104,'canny',32,1235);

select * from sll;

set serveroutput on;

declare

v_e_eid sll.eid%type;
v_e_ename sll.ename%type;
v_e_age sll.age%type;
v_e_salary sll.salary%type;

cursor sl_cursor is
select eid,ename,age,salary from sll;
begin
open sl_cursor;
loop
fetch sl_cursor into v_e_eid,v_e_ename,v_e_age,v_e_salary;
exit when sl_cursor%notfound;
dbms_output.put_line('Employee id:'||v_e_eid);
dbms_output.put_line('Employee name:'||v_e_ename);
dbms_output.put_line('Employee age:'||v_e_age);
dbms_output.put_line('Employee salary:'||v_e_salary);
dbms_output.put_line('--------------------------------');
end loop;
close sl_cursor;
exception
when others then
dbms_output.put_line('Error occurred:'||sqlerrm);
if sl_cursor%isopen then
close sl_cursor;
end if;
end;
